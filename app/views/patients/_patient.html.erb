
<div id="patientDivId">
<table class="table table-striped table-bordered table-condensed">
  <thead>
  <tr>
    <th> Registered_on </th>
    <th> Name </th>
    <%# <th> Email</th> %>
    <th> Phone Number </th>
    <th> Age </th>
    <th> Gender </th>
    <th> County </th>
    <th> Town </th>
  </tr>
  </thead>
  <% if patients == nil || patients.size == 0 %>
      <tr>
        <td colspan="6" style="text-align: center;">No records found.</td>
      </tr>
  <% else%>
      <% patients.each do |patient|%>
          <tr>
            <td><%= patient.created_at%></td>
            <td><%= link_to "#{patient.name}", patient%></td>
            <%# <td><%= patient.email%></td> %>
            <td><%= patient.phone_number%></td>
            <td><%= patient.age %></td>
            <td> <%= patient.gender %> </td>
            <td> <%= patient.county %> </td>
            <td> <%= patient.town %> </td>
          </tr>
      <% end %>
  <% end %>

</table>
  <% if patients != nil && !patients.empty? %>
    <%= will_paginate patients%>
  <% end %>

</div>
